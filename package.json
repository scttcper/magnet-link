{
  "name": "@ctrl/magnet-link",
  "version": "0.0.0-placeholder",
  "description": "Parse a magnet URI into an object",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "homepage": "https://magnet-link.vercel.app",
  "license": "MIT",
  "repository": "scttcper/magnet-link",
  "keywords": [
    "torrent",
    "magnet",
    "link",
    "magnet-uri"
  ],
  "type": "module",
  "exports": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "scripts": {
    "demo:build": "npm run build --workspace=demo",
    "demo:watch": "npm run dev --workspace=demo",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prepare": "npm run build",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=./junit.xml"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^2.1.1"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.4.4",
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/node": "17.0.39",
    "buffer": "6.0.3",
    "c8": "7.11.3",
    "typescript": "4.7.3",
    "vitest": "0.13.1"
  },
  "workspaces": [
    "demo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "engines": {
    "node": ">=14.16"
  }
}
